{
  "name": "@merit-systems/x402-server-telemetry",
  "version": "0.1.2",
  "description": "Telemetry package for x402-protected API servers. Extracts identity headers, logs invocations to ClickHouse, and extracts verified wallet addresses from x402 payments and SIWX auth.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.ts' '*.json' '*.mjs'",
    "format:check": "prettier --check 'src/**/*.ts' '*.json' '*.mjs'",
    "typecheck": "tsc --noEmit",
    "check": "npm run format:check && npm run lint && npm run typecheck && npm run build",
    "prepublishOnly": "npm run check"
  },
  "keywords": [
    "x402",
    "telemetry",
    "clickhouse",
    "merit-systems"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@clickhouse/client": "^1.12.0",
    "@x402/core": "^2.2.0",
    "@x402/extensions": "^2.3.0",
    "@x402/next": "^2.2.0",
    "next": ">=15.0.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@x402/next": {
      "optional": true
    },
    "@x402/extensions": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@clickhouse/client": "^1.16.0",
    "@eslint/js": "^9.0.0",
    "@x402/core": "^2.2.0",
    "@x402/extensions": "^2.3.0",
    "@x402/next": "^2.2.0",
    "eslint": "^9.0.0",
    "next": "16.0.10",
    "prettier": "^3.8.1",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.55.0",
    "zod": "^4.1.13"
  }
}
